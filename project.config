[access]
	inheritFrom = All-Projects
[submit]
	rejectEmptyCommit = true
	action = fast forward only
[project]
	description = Sandbox
[access "refs/*"]
	owner = group polska-mistrzem-polski
	owner = group user/Michal Krawczyk (semihalf-krawczyk-michal)
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group polska-mistrzem-polski
	label-Code-Review = -2..+2 group user/Michal Krawczyk (semihalf-krawczyk-michal)
	label-Verified = -1..+1 group polska-mistrzem-polski
	label-Verified = -1..+1 group user/Michal Krawczyk (semihalf-krawczyk-michal)
	push = group polska-mistrzem-polski
	push = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	read = group polska-mistrzem-polski
	read = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	rebase = group polska-mistrzem-polski
	rebase = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	removeReviewer = group polska-mistrzem-polski
	removeReviewer = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	submit = group polska-mistrzem-polski
	submit = group user/Michal Krawczyk (semihalf-krawczyk-michal)
[access "refs/heads/*"]
	create = group polska-mistrzem-polski
	create = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	push = group polska-mistrzem-polski
	push = +force group user/Michal Krawczyk (semihalf-krawczyk-michal)
	pushMerge = group polska-mistrzem-polski
	pushMerge = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	read = group polska-mistrzem-polski
	read = group user/Michal Krawczyk (semihalf-krawczyk-michal)
[access "refs/tags/*"]
	push = group polska-mistrzem-polski
	push = group user/Michal Krawczyk (semihalf-krawczyk-michal)
	push = +force group user/Michal Krawczyk (semihalf-krawczyk-michal)
[receive]
	requireChangeId = true
